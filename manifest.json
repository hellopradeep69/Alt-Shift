                "default":"Alt+Shift+3"
            },
            "description":"Move tab to 3"
        },
        "Alt_shift_4":{
            "suggested_key":{
                "default":"Alt+Shift+4"
            },
            "description":"Move tab to 4"
        },
        "Alt_shift_5":{
            "suggested_key":{
                "default":"Alt+Shift+5"
            },
            "description":"Move tab to 5"
        }

    }

}
